func inc(number steps)
begin
    for i from 1 to steps do
        forward(i);
    end;
end;

func circle(number radio)
begin
    arc(360, radio);
end;

program
    writeln "Insert a number : ";
    with
        number n;
    do
        read n; # I suggest that this number be a multiple of 360
        with
            number angle = n;
        do
            while angle <= 360 do
                rotater(n);
                circle(100);
                angle = angle + n;
            end;
        end;
    end;
end;